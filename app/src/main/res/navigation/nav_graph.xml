<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
        app:startDestination="@id/listaImagemPesquisadaFragment">

    <fragment
        android:id="@+id/NotaViewPagerFragment"
        android:name="com.example.myapplication.view.nota.NotaViewPagerFragment"
        android:label="@string/nota_view_pager_label"
        tools:layout="@layout/fragment_nota_view_pager" >
        <action
                android:id="@+id/action_voltarParaListaNotas"
                app:destination="@id/listaImagemPesquisadaFragment" />
        <argument
                android:name="posicao"
                app:argType="integer" />
        <argument
                android:name="isNotaNova"
                app:argType="boolean"  />
    </fragment>
    <fragment
            android:id="@+id/listaImagemPesquisadaFragment"
            android:name="com.example.myapplication.view.listaimageminicial.ListaImagemPesquisadaFragment"
            android:label="fragment_imagem_item_list"
            tools:layout="@layout/fragment_imagem_item_list" >
        <action
                android:id="@+id/action_criar_nota_imagem"
                app:destination="@id/NotaViewPagerFragment" />
    </fragment>
    <fragment
            android:id="@+id/sobreFragment"
            android:name="com.example.myapplication.view.sobre.SobreFragment"
            android:label="fragment_sobre"
            tools:layout="@layout/fragment_sobre" />
</navigation>